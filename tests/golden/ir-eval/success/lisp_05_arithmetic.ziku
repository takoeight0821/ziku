-- Arithmetic operations on S-expression numbers
let addNum = \a => \b =>
  match a {
  | Num(x) => match b {
             | Num(y) => Num(x + y)
             | Nil => Nil
             | Cons(h, t) => Nil
}
  | Nil => Nil
  | Cons(h, t) => Nil
}
in
let mulNum = \a => \b =>
  match a {
  | Num(x) => match b {
             | Num(y) => Num(x * y)
             | Nil => Nil
             | Cons(h, t) => Nil
}
  | Nil => Nil
  | Cons(h, t) => Nil
}
in
let num1 = Num(3) in
let num2 = Num(4) in
match addNum num1 num2 {
| Num(n) => n
| Nil => -1
| Cons(h, t) => -2
}
