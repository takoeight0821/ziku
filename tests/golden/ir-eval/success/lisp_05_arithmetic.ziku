-- Arithmetic operations on S-expression numbers
let addNum = \a => \b =>
  match a with
  | Num(x) => match b with
             | Num(y) => Num(x + y)
             | Nil => Nil
             | Cons(h, t) => Nil
             end
  | Nil => Nil
  | Cons(h, t) => Nil
  end
in
let mulNum = \a => \b =>
  match a with
  | Num(x) => match b with
             | Num(y) => Num(x * y)
             | Nil => Nil
             | Cons(h, t) => Nil
             end
  | Nil => Nil
  | Cons(h, t) => Nil
  end
in
let num1 = Num(3) in
let num2 = Num(4) in
match addNum num1 num2 with
| Num(n) => n
| Nil => -1
| Cons(h, t) => -2
end
