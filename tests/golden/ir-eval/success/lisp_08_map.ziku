-- Map function on S-expression lists
let rec lispMap = \f => \lst =>
  match lst {
  | Nil => Nil
  | Cons(h, t) => Cons(f h, lispMap f t)
  | Num(n) => Nil
}
in
-- Double each number
let double = \x =>
  match x {
  | Num(n) => Num(n * 2)
  | Nil => Nil
  | Cons(h, t) => Nil
}
in
let list1 = Cons(Num(5), Nil) in
-- Get first element of doubled list (5 * 2 = 10)
let getCar = \lst =>
  match lst {
  | Cons(h, t) => h
  | Nil => Nil
  | Num(n) => Nil
}
in
match getCar (lispMap double list1) {
| Num(n) => n
| Nil => -1
| Cons(h, t) => -2
}
