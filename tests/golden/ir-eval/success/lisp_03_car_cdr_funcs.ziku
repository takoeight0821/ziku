-- Car and cdr as functions (using different names to avoid Scheme shadowing)
let getCar = \lst =>
  match lst {
  | Cons(h, t) => h
  | Nil => Nil
}
in
let getCdr = \lst =>
  match lst {
  | Cons(h, t) => t
  | Nil => Nil
}
in
let list1 = Cons(Num(1), Cons(Num(2), Nil)) in
match getCar list1 {
| Num(n) => n
| Nil => -1
| Cons(h, t) => -2
}
